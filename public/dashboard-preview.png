<svg width="800" height="500" viewBox="0 0 800 500" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="500" rx="16" fill="url(#bgGradient)"/>

  <!-- Header -->
  <rect x="20" y="20" width="760" height="60" rx="8" fill="rgba(255,255,255,0.1)"/>
  <circle cx="50" cy="50" r="15" fill="url(#iconGradient)"/>
  <rect x="80" y="40" width="120" height="8" rx="4" fill="rgba(255,255,255,0.3)"/>
  <rect x="80" y="55" width="80" height="6" rx="3" fill="rgba(255,255,255,0.2)"/>

  <!-- Sidebar -->
  <rect x="20" y="100" width="200" height="380" rx="8" fill="rgba(255,255,255,0.05)"/>
  <rect x="35" y="120" width="170" height="30" rx="6" fill="url(#menuGradient)"/>
  <rect x="35" y="160" width="170" height="30" rx="6" fill="rgba(255,255,255,0.05)"/>
  <rect x="35" y="200" width="170" height="30" rx="6" fill="rgba(255,255,255,0.05)"/>
  <rect x="35" y="240" width="170" height="30" rx="6" fill="rgba(255,255,255,0.05)"/>

  <!-- Main Content Area -->
  <rect x="240" y="100" width="540" height="180" rx="8" fill="rgba(255,255,255,0.08)"/>
  <rect x="260" y="120" width="200" height="12" rx="6" fill="rgba(255,255,255,0.3)"/>
  <rect x="260" y="145" width="300" height="8" rx="4" fill="rgba(255,255,255,0.2)"/>

  <!-- Cards Grid -->
  <rect x="240" y="300" width="250" height="180" rx="8" fill="rgba(255,255,255,0.05)"/>
  <rect x="510" y="300" width="270" height="180" rx="8" fill="rgba(255,255,255,0.05)"/>

  <!-- Chart in first card -->
  <path d="M260 420 L280 400 L300 410 L320 390 L340 400 L360 380 L380 395 L400 370 L420 380 L440 360 L460 375"
        stroke="url(#chartGradient)" stroke-width="3" fill="none" stroke-linecap="round"/>

  <!-- Stats in second card -->
  <circle cx="540" cy="340" r="30" fill="url(#statGradient)"/>
  <rect x="590" y="330" width="150" height="10" rx="5" fill="rgba(255,255,255,0.3)"/>
  <rect x="590" y="350" width="100" height="8" rx="4" fill="rgba(255,255,255,0.2)"/>

  <defs>
    <linearGradient id="bgGradient" x1="0" y1="0" x2="800" y2="500">
      <stop offset="0" stop-color="#1e1b4b"/>
      <stop offset="1" stop-color="#0f172a"/>
    </linearGradient>
    <linearGradient id="iconGradient" x1="35" y1="35" x2="65" y2="65">
      <stop offset="0" stop-color="#8b5cf6"/>
      <stop offset="1" stop-color="#6366f1"/>
    </linearGradient>
    <linearGradient id="menuGradient" x1="35" y1="120" x2="205" y2="150">
      <stop offset="0" stop-color="rgba(139,92,246,0.3)"/>
      <stop offset="1" stop-color="rgba(99,102,241,0.3)"/>
    </linearGradient>
    <linearGradient id="chartGradient" x1="260" y1="360" x2="460" y2="420">
      <stop offset="0" stop-color="#8b5cf6"/>
      <stop offset="1" stop-color="#6366f1"/>
    </linearGradient>
    <linearGradient id="statGradient" x1="510" y1="310" x2="570" y2="370">
      <stop offset="0" stop-color="#8b5cf6"/>
      <stop offset="1" stop-color="#6366f1"/>
    </linearGradient>
  </defs>
</svg>
